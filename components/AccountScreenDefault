if (signUp){
            return(
                <View style={{justifyContent:'center',alignItems:'center'}}>
                    <Text style={{margin:15}} category={'h6'}>(Sign-up/Sign-in)</Text>
                    <Switch value={signUp} onValueChange={() => {setSignUp(!signUp)}}></Switch>
                    <LoginPage LoginWithEmailandPassword={LoginWithEmailandPassword}></LoginPage>
                </View>
            )
        }
        else {
            return(
                <View  style={{justifyContent:'center',alignItems:'center'}}>
                    <Text style={{margin:15}} category={'h6'}>(Sign-up/Sign-in)</Text>
                    <Switch value={signUp} onValueChange={() => {setSignUp(!signUp)}}></Switch>

                    <SignupPage SignUpWithEmailAndPassword={SignUpWithEmailAndPassword}>

                    </SignupPage>
                </View>
            )
        }